        "+ ",
        "             ))",
        "+ ",
        "  )",
        "+ ",
        "}",
        "> ",
        "",
        "> ",
        "# Chunk 7",
        "> ",
        "app_server <- function(input, output, session) {",
        "+ ",
        "  1 <- reactiveValues()",
        "+ ",
        "  choice_server(\"choice_ui_1\", r = 1)",
        "+ ",
        "  printing_server(\"printing_ui_1\", r = 1)",
